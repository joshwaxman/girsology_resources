      "gemara_segment_ids": [
        0
      ]
    },
    {
      "width": "250px",
      "right": "0px",
      "left": "",
      "text": "<b>ומצא</b> <b>אחד</b> <b>בתוך</b> <b>עשרים</b> <b>בודק</b> <b>ממנו</b>",
      "source_ref": "Rashi on Nazir 65a:1:1",
