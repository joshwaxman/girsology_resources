      "gemara_segment_ids": [
        1
      ]
    },
    {
      "width": "250px",
      "right": "0px",
      "left": "",
      "text": "ויחזור לאחוריו: <b>אִי יָהֵיב דַּעְתֵּיהּ</b>",
      "source_ref": "Rashi on Shabbat 110a:3:1",
      "gemara_segment_ids": [
        2
      ]
    },
    {
